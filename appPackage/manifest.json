{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.15/MicrosoftTeams.schema.json",
  "manifestVersion": "1.15",
  "version": "1.0.0",
  "id": "${{TEAMS_APP_ID}}",
  "packageName": "com.microsoft.teams.extension",
  "developer": {
    "name": "Prakash Softwares Pvt.Ltd",
    "websiteUrl": "https://prakashinfotech.com",
    "privacyUrl": "https://prakashinfotech.com",
    "termsOfUseUrl": "https://prakashinfotech.com"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "teamsApp-msg-ext-${{TEAMSFX_ENV}}",
    "full": "Full name for teamsApp-msg-ext"
  },
  "description": {
    "short": "Short description of teamsApp-msg-ext",
    "full": "Full description of teamsApp-msg-ext"
  },
  "accentColor": "#FFFFFF",
  "bots": [],
  "composeExtensions": [],
  "configurableTabs": [
    {
      "configurationUrl": "${{TAB_ENDPOINT}}/index.html#/config",
      "canUpdateConfiguration": true,
      "scopes": ["team", "groupchat"],
      "context": ["meetingStage", "meetingSidePanel", "meetingDetailsTab"],
      "meetingSurfaces": ["sidePanel", "stage"]
    }
  ],
  "staticTabs": [
    {
      "entityId": "index",
      "name": "Questionnaire",
      "contentUrl": "${{TAB_ENDPOINT}}/index.html#/tab",
      "websiteUrl": "${{TAB_ENDPOINT}}/index.html#/tab",
      "scopes": ["personal"]
    },
    {
      "entityId": "analytics",
      "name": "Analytics",
      "contentUrl": "${{TAB_ENDPOINT}}/index.html#/analytics",
      "websiteUrl": "${{TAB_ENDPOINT}}/index.html#/analytics",
      "scopes": ["personal"]
    }
  ],
  "authorization": {
    "permissions": {
      "resourceSpecific": [
        {
          "name": "MeetingStage.Write.Chat",
          "type": "Delegated"
        },
        {
          "name": "ChannelMeetingStage.Write.Group",
          "type": "Delegated"
        },
        {
          "name": "LiveShareSession.ReadWrite.Chat",
          "type": "Delegated"
        },
        {
          "name": "LiveShareSession.ReadWrite.Group",
          "type": "Delegated"
        }
      ]
    }
  },
  "permissions": ["identity", "messageTeamMembers"],
  "validDomains": ["${{TAB_DOMAIN}}", "*.mehidpurwala.xyz"],
	"isFullScreen": true,
  "showLoadingIndicator": false,
  "webApplicationInfo": {
    "id": "${{AAD_APP_CLIENT_ID}}",
    "resource": "api://${{TAB_DOMAIN}}/${{AAD_APP_CLIENT_ID}}"
  }
}
